{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "backtick"],
        "linebreak-style": ["error", "windows"],
        "prettier/prettier": "error"
    }
}
